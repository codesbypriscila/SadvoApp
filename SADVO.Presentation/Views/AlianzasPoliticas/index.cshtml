                </h2>
                <p class="text-gray-600 mt-1">Alianzas políticas vigentes de tu partido</p>
            </div>

            @if (ViewBag.AlianzasActivas != null && ((IEnumerable<SADVO.Application.Dtos.AlianzaPoliticaDto>)ViewBag.AlianzasActivas).Any())
            {
                <div class="overflow-x-auto">
                    <table class="min-w-full divide-y divide-gray-200">
                        <thead class="bg-gray-50">
                            <tr>
                                <th class="px-6 py-4 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
