                </div>
                     </th>
                     </tr>
                </thead>
<tbody class="bg-white divide-y divide-gray-200">
@foreach (var alianza in (IEnumerable<SADVO.Application.Dtos.AlianzaPoliticaDto>)ViewBag.AlianzasActivas)
{
    <tr class="hover:bg-gray-50 transition-colors duration-150">

        <td class="px-6 py-4 whitespace-nowrap">
            <div class="text-sm font-medium text-gray-900">
                @(alianza.PartidoSolicitanteId == (int)ViewBag.PartidoId ? alianza.PartidoReceptorNombre : alianza.PartidoSolicitanteNombre)
            </div>
            <div class="text-sm text-gray-500 font-mono bg-green-50 px-2 py-1 rounded inline-block mt-1">
                @(alianza.PartidoSolicitanteId == (int)ViewBag.PartidoId ? alianza.PartidoReceptorSiglas : alianza.PartidoSolicitanteSiglas)
            </div>
        </td>

        <td class="px-6 py-4 whitespace-nowrap">
            @if (alianza.FechaRespuesta.HasValue)
            {
                <div class="text-sm text-gray-900">@alianza.FechaRespuesta.Value.ToString("dd/MM/yyyy")</div>
                <div class="text-xs text-gray-500">@alianza.FechaRespuesta.Value.ToString("HH:mm")</div>
            }
            else
            {
                <div class="text-sm text-gray-400 italic">Sin respuesta</div>
            }
        </td>
    </tr>
}
</tbody>

                               </table>
                                    </div>
